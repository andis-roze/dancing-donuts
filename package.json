{
  "name": "dancingdonuts",
  "version": "0.0.1",
  "description": "Dancing donuts",
  "main": "index.ts",
  "author": "Andis Roze",
  "license": "MIT",
  "scripts": {
    "dist:dev": "webpack --env.NODE_ENV=dev --config ./webpack.config.js",
    "tslint": "tslint 'src/**/*.ts' --format stylish",
    "watch": "webpack --env.NODE_ENV=dev --watch",
    "start": "webpack-dev-server --env.NODE_ENV=dev",
    "test": "JEST_JUNIT_OUTPUT=./test-results/junit.xml jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "src/.*\\.spec\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "dependencies": {
    "gl-mat4": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "autoprefixer": "^9.3.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "raw-loader": "^1.0.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "engines": {
    "yarn": "^1.12.1"
  }
}
