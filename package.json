{
  "name": "dancingdonuts",
  "version": "0.0.1",
  "description": "Dancing donuts",
  "main": "index.ts",
  "author": "Andis Roze",
  "license": "MIT",
  "scripts": {
    "dist:dev": "webpack --env.NODE_ENV=dev --config ./webpack.config.js",
    "tslint": "tslint 'src/**/*.ts' --format stylish",
    "watch": "webpack --env.NODE_ENV=dev --watch",
    "start": "webpack-dev-server --env.NODE_ENV=dev",
    "test": "JEST_JUNIT_OUTPUT=./test-results/junit.xml jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "src/.*\\.spec\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "dependencies": {
    "gl-matrix": "^3.0.0"
  },
  "devDependencies": {
    "@types/gl-matrix": "^2.4.5",
    "@types/jest": "^24.0.15",
    "autoprefixer": "^9.6.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "raw-loader": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "engines": {
    "yarn": "^1.12.1"
  }
}
